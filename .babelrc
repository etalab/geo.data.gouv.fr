{
  "presets": [
    ["next/babel", {
      "styled-jsx": {
        "plugins": [
          ["styled-jsx-plugin-sass", {
            "sassOptions": {
              "includePaths": ["styles"],
              "precision": 2
            }
          }]
        ]
      }
    }]
  ],
  "plugins": [
    ["inline-import", {
      "extensions": [".css"]
    }]
  ]
}
