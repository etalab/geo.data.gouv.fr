{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      },
      "loose": true,
      "modules": false
    }]
  ],

  "plugins":[
    "transform-class-properties",
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    ["transform-react-jsx", {
      "useBuiltIns": true
    }],
    "transform-react-display-name",
    "lodash",
    "dynamic-import-node",
    ["import-inspector", {
      "serverSideRequirePath": true,
      "webpackRequireWeakId": true
    }]
  ]
}
