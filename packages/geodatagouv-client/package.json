{
  "name": "geodatagouv-client",
  "version": "2.1.3",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node build/scripts/start",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "test": "cross-env NODE_ENV=test mocha -r babel-register -r build/tests/bootstrap.js src/**/*.test.js",
    "test:ci": "cross-env NODE_ENV=test MOCHA_FILE=reports/tests/geodatagouv/results.xml nyc mocha -r build/tests/bootstrap.js --reporter mocha-circleci-reporter src/**/*.test.js",
    "codecov": "codecov",
    "deploy": "gh-pages -d dist -m 'Deploy [skip ci]'",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint **/*.scss",
    "heroku-postbuild": "yarn install --production=false && npm run build"
  },
  "devDependencies": {
    "bluebird": "^3.5.0",
    "chai": "^4.1.1",
    "chai-enzyme": "^0.8.0",
    "chalk": "^2.1.0",
    "classnames": "^2.2.5",
    "codecov": "^2.3.0",
    "color": "^2.0.0",
    "compression": "^1.7.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "enzyme": "^2.4.1",
    "eslint": "^4.5.0",
    "expect": "^1.20.2",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.1",
    "gh-pages": "^1.0.0",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.1",
    "ignore-styles": "^5.0.1",
    "imagemin-loader": "^1.0.6",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^5.2.2",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "lol": "^1.0.101-lol",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "mocha-circleci-reporter": "^0.0.2",
    "module-alias": "^2.0.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.1.0",
    "proxyquire": "^1.7.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "^15.6.1",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-recommended": "^1.0.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "unused-files-webpack-plugin": "^3.0.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "chart.js": "^2.5.0",
    "hoist-non-react-statics": "^2.3.0",
    "i18next": "^9.0.0",
    "i18next-browser-languagedetector": "^2.0.0",
    "leaflet": "^1.2.0",
    "lodash-es": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.14.1",
    "piwik-react-router": "^0.9.0",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "react-addons-create-fragment": "^15.4.0",
    "react-chartjs-2": "^2.6.1",
    "react-helmet": "^5.1.3",
    "react-i18next": "^5.2.0",
    "react-leaflet": "^1.6.4",
    "react-loadable": "^4.0.5",
    "react-loadable-visibility": "^2.3.0",
    "react-paginate": "^4.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactable": "^0.14.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "2.2.10",
    "underscore.string": "^3.3.4"
  },
  "engines": {
    "node": "8.x"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/__test__/**",
      "**/__mocks__/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
