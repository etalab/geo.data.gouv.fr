    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "chart.js": "^2.5.0",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "leaflet": "^1.0.2",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "moment": "^2.14.1",
    "piwik-react-router": "^0.9.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.9.4",
    "react-addons-create-fragment": "^15.4.0",
    "react-chartjs-2": "^2.0.0",
    "react-document-title": "^2.0.2",
    "react-i18next": "^4.8.0",
    "react-leaflet": "^1.4.1",
    "react-paginate": "^4.1.1",
    "react-redux": "^5.0.5",
    "react-router": "3.0.3",
    "react-router-scroll": "^0.4.1",
    "react-sticky": "^5.0.5",
    "reactable": "^0.14.1",
