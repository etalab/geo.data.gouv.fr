    "redux-thunk": "^2.2.0",
    "underscore.string": "^3.3.4"
  },
  "engines": {
    "node": "8.x"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/__test__/**",
      "**/__mocks__/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
