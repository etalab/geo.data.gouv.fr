{
  "name": "geo.data.gouv.fr",
  "private": true,
  "version": "3.7.1",
  "license": "MIT",
  "repository": "git@github.com:etalab/geo.data.gouv.fr.git",
  "scripts": {
    "dev": "node server",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "npm run lint:scripts",
    "lint:scripts": "xo",
    "heroku-postbuild": "yarn install --production=false && yarn build"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/flip": "^5.1.5",
    "bytes": "^3.0.0",
    "chart.js": "^2.7.2",
    "compression": "^1.7.2",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "hoist-non-react-statics": "^3.0.1",
    "i18next": "^11.3.3",
    "i18next-express-middleware": "^1.1.1",
    "i18next-node-fs-backend": "^2.0.0",
    "i18next-xhr-backend": "^1.5.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "mapbox-gl": "^0.47.0",
    "mapbox-omt-lang": "^1.1.0",
    "marked": "^0.5.0",
    "moment": "^2.22.2",
    "next": "^7.0.1",
    "next-runtime-dotenv": "^1.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-chartjs-2": "^2.7.2",
    "react-dom": "^16.5.2",
    "react-i18next": "^7.7.0",
    "react-icons": "^2.2.7",
    "react-table": "^6.8.6"
  },
  "resolutions": {
    "chart.js/moment": "^2.22.2",
    "@turf/helpers": "^6.x",
    "@turf/meta": "^6.x"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.0.1",
    "babel-plugin-inline-import": "^3.0.0",
    "codecov": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.1.0",
    "eslint-config-xo-nextjs": "^1.3.0",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.1.0",
    "jest-junit": "^5.1.0",
    "lerna-changelog": "^0.8.0",
    "mock-local-storage": "^1.0.5",
    "node-sass": "^4.9.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.4.1",
    "styled-jsx": "^3.0.2",
    "styled-jsx-plugin-sass": "^0.3.0",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "xo": "^0.23.0"
  },
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": "8.x",
    "yarn": "1.x"
  },
  "jest": {
    "verbose": true,
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "reports/tests/junit.xml"
        }
      ]
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "components/**/*.js",
      "lib/**/*.js",
      "server/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  },
  "xo": {
    "extends": "xo-nextjs",
    "semicolon": false,
    "space": 2,
    "env": "browser",
    "rules": {
      "camelcase": "warn"
    },
    "overrides": [
      {
        "files": "__tests__/**/*.js",
        "env": [
          "browser",
          "jest"
        ]
      }
    ]
  }
}
